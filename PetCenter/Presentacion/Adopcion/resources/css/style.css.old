/* --------------------- paginacion -----------------------*/
.pagination2 {
    display: inline-block;
    padding-left: 0;
    margin: 20px 0;
    border-radius: 4px;
}
.pagination2>li {
    display: inline;
}
.pagination2>.active>a, 
.pagination2>.active>a:focus, 
.pagination2>.active>a:hover, 
.pagination2>.active>span,
.pagination2>.active>span:focus,
.pagination2>.active>span:hover {
    z-index: 3;
    color: #fff;
    cursor: default;
    background-color: #777777;
    border-color: #fff;
}
.pagination2>li>a, .pagination2>li>span {
    position: relative;
    float: left;
    padding: 8px 14px;
    margin-left: -1px;
    line-height: 1.42857143;
    color: #fff;
    text-decoration: none;
    background-color: #9A1413;
    border: 1px solid #fff;
}

